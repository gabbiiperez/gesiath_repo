<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_view_form_customer_info_classa1" model="ir.ui.view">
        <field name="name">res.partner.view.form.customer.info.classa1</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="age" string="Age of partner"/>
                <field name="gender"/>
                <field name="description"/>
            </xpath>
        </field>
    </record>
</odoo>
